@startuml

!if %variable_exists("$TITLE")
title $TITLE
!else
title "Usecase Diagram"
!endif

left to right direction
'skinparam packageStyle rectangle
actor customer
actor clerk
rectangle checkout {
  customer -- (checkout)
  (checkout) .> (payment) : include
  (help) .> (checkout) : extends
  (checkout) -- clerk
}
@enduml